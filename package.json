    "js-cookie": "^3.0.5",
    "less": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.29.7",
    "@types/js-cookie": "^3.0.3",
